@import "../../../assets/css/variable.styl"

.modal-box
  position absolute
  top 50%
  left 50%
  margin-top -80px
  margin-left -49px
  width 160px
  height 98px
  text-align center
  background rgba(52,52,52,0.6)
  border-radius 6px
  padding 12px 0 14px
  z-index 2
  svg
